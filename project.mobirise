          "media": {
            "image": {
              "active": true,
              "width": 1400,
              "src": "@PROJECT_PATH@/assets/images/matrix-valve.jpg",
              "link": "",
              "alt": "cabin heater matrix valve pch-oo3300",
              "title": "",
